<?xml version="1.0" encoding="utf-8"?><openerp><data>    <!-- is.taux.rotation.stock.new -->    <record id="is_taux_rotation_stock_new_form_view" model="ir.ui.view">        <field name="name">is_taux_rotation_stock_new_form_view</field>        <field name="model">is.taux.rotation.stock.new</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="product_id"/>                            <field name="cat"/>                            <field name="gest"/>                            <field name="cout_act"/>                            <field name="stock"/>                            <field name="pic_12mois"/>                            <field name="pic_3mois_ferme"/>                            <field name="pic_3mois_prev"/>                            <field name="fm_3mois"/>                            <field name="ft_3mois"/>                            <field name="besoin_total"/>                            <field name="nb_sem"/>                            <field name="stock_valorise"/>                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_taux_rotation_stock_new_tree_view" model="ir.ui.view">        <field name="name">is_taux_rotation_stock_new_tree_view</field>        <field name="model">is.taux.rotation.stock.new</field>        <field name="arch" type="xml">            <tree create="false">                <field name="code_pg"/>                <field name="designation"/>                <field name="cat"/>                <field name="gest"/>                <field name="cout_act"/>                <field name="stock"/>                <field name="pic_12mois"/>                <field name="pic_3mois_ferme"/>                <field name="pic_3mois_prev"/>                <field name="fm_3mois"/>                <field name="ft_3mois"/>                <field name="besoin_total"/>                <field name="nb_sem"/>                <field name="stock_valorise"/>           </tree>        </field>    </record>    <record id="is_taux_rotation_stock_new_search_view" model="ir.ui.view">        <field name="name">is_taux_rotation_stock_new_search_view</field>        <field name="model">is.taux.rotation.stock.new</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id"/>                <field name="cat"/>                <field name="gest"/>                <group expand="0" string="Regrouper par...">                    <filter string="Catégorie"          context="{'group_by':'cat'}"/>                    <filter string="Gestionnaire"       context="{'group_by':'gest'}"/>                </group>            </search>        </field>    </record>    <record id="is_taux_rotation_stock_new_action" model="ir.actions.act_window">        <field name="name">Taux de rotation des stocks</field>        <field name="res_model">is.taux.rotation.stock.new</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record>    <record forcecreate="True" id="is_taux_rotation_stock_new_scheduler_action" model="ir.cron">        <field name="name">REFRESH is_taux_rotation_stock_new</field>        <field name="active" eval="True" />        <field name="user_id" ref="base.user_root"/>        <field name="interval_number">30</field>        <field name="interval_type">minutes</field>        <field name="numbercall">-1</field>        <field eval="False" name="doall"/>        <field eval="'is.taux.rotation.stock.new'" name="model"/>        <field eval="'run_is_taux_rotation_stock_new_scheduler_action'" name="function"/>        <field eval="'(True,)'" name="args"/>    </record>    <!-- is.taux.rotation.stock.view -->    <record id="is_taux_rotation_stock_view_form_view" model="ir.ui.view">        <field name="name">is_taux_rotation_stock_view_form_view</field>        <field name="model">is.taux.rotation.stock.view</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="product_id"/>                            <field name="cat"/>                            <field name="gest"/>                            <field name="cout_act"/>                            <field name="stock"/>                            <field name="pic_12mois"/>                            <field name="pic_3mois_ferme"/>                            <field name="pic_3mois_prev"/>                            <field name="fm_3mois"/>                            <field name="ft_3mois"/>                            <field name="besoin_total"/>                            <field name="nb_sem"/>                            <field name="stock_valorise"/>                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_taux_rotation_stock_view_tree_view" model="ir.ui.view">        <field name="name">is_taux_rotation_stock_view_tree_view</field>        <field name="model">is.taux.rotation.stock.view</field>        <field name="arch" type="xml">            <tree create="false">                <field name="code_pg"/>                <field name="designation"/>                <field name="cat"/>                <field name="gest"/>                <field name="cout_act"/>                <field name="stock"/>                <field name="pic_12mois"/>                <field name="pic_3mois_ferme"/>                <field name="pic_3mois_prev"/>                <field name="fm_3mois"/>                <field name="ft_3mois"/>                <field name="besoin_total"/>                <field name="nb_sem"/>                <field name="stock_valorise"/>           </tree>        </field>    </record>    <record id="is_taux_rotation_stock_view_search_view" model="ir.ui.view">        <field name="name">is_taux_rotation_stock_view_search_view</field>        <field name="model">is.taux.rotation.stock.view</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id"/>                <field name="cat"/>                <field name="gest"/>                <group expand="0" string="Regrouper par...">                    <filter string="Catégorie"          context="{'group_by':'cat'}"/>                    <filter string="Gestionnaire"       context="{'group_by':'gest'}"/>                </group>            </search>        </field>    </record>    <record id="is_taux_rotation_stock_view_action" model="ir.actions.act_window">        <field name="name">Taux de rotation des stocks</field>        <field name="res_model">is.taux.rotation.stock.view</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record></data></openerp>