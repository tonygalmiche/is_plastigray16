<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_comparatif_article_tarif_cial_form_view" model="ir.ui.view">        <field name="name">is_comparatif_article_tarif_cial_form_view</field>        <field name="model">is.comparatif.article.tarif.cial</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="product_id"/>                            <field name="segment_id"/>                            <field name="is_category_id"/>                            <field name="is_gestionnaire_id"/>                            <field name="client_id"/>                            <field name="tarif_cial_id"/>                        </group>                     </group>                </sheet>            </form>        </field>    </record>    <record id="is_comparatif_article_tarif_cial_tree_view" model="ir.ui.view">        <field name="name">is_comparatif_article_tarif_cial_tree_view</field>        <field name="model">is.comparatif.article.tarif.cial</field>        <field name="arch" type="xml">            <tree create="false">                <field name="product_id"           widget="many2one_clickable"/>                <field name="segment_id"           widget="many2one_clickable"/>                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <field name="client_id"            widget="many2one_clickable"/>                <field name="tarif_cial_id"        widget="many2one_clickable"/>           </tree>        </field>    </record>    <record id="is_comparatif_article_tarif_cial_search_view" model="ir.ui.view">        <field name="name">is_comparatif_article_tarif_cial_search_view</field>        <field name="model">is.comparatif.article.tarif.cial</field>        <field name="arch" type="xml">            <search string="Recherche">                <filter string="Articles sans Tarif" domain="[('tarif_cial_id','=', False)]" name="anomalies" />                <field name="product_id"/>                <field name="segment_id"/>                <field name="is_category_id"/>                <field name="is_gestionnaire_id"/>                <field name="client_id"/>                <group expand="0" string="Regrouper par...">                    <filter string="Segment"      context="{'group_by':'segment_id'}"/>                    <filter string="CatÃ©gorie"    context="{'group_by':'is_category_id'}"/>                    <filter string="Gestionnaire" context="{'group_by':'is_gestionnaire_id'}"/>                    <filter string="Client"       context="{'group_by':'client_id'}"/>                </group>            </search>        </field>    </record>    <record id="is_comparatif_article_tarif_cial_action" model="ir.actions.act_window">        <field name="name">Comparatif articles / Tarif Commercial</field>        <field name="res_model">is.comparatif.article.tarif.cial</field>        <field name="domain">[            ('segment_id.name','like','PRODUIT FINI')        ]</field>        <field name="context">{"search_default_anomalies":1}</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record></data></openerp>