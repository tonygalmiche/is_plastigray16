<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_comparatif_tarif_facture_tree_view" model="ir.ui.view">        <field name="name">is_comparatif_tarif_facture_tree_view</field>        <field name="model">is.comparatif.tarif.facture</field>        <field name="arch" type="xml">            <tree colors="red:price_delta != 0" create="false">                <field name="invoice_id"         widget="many2one_clickable"  />                <field name="invoice_date" />                <field name="order_id"           widget="many2one_clickable"  />                <field name="partner_id"         widget="many2one_clickable"  />                <field name="pricelist_id"       widget="many2one_clickable"  />                <field name="product_id"         widget="many2one_clickable"  />                <field name="quantity"           digits="(14, 4)"/>                <field name="uos_id" />                <field name="invoice_price"      digits="(14, 4)"/>                <field name="pricelist_price"    digits="(14, 4)"/>                <field name="price_delta"        digits="(14, 4)"/>                <field name="lot_livraison"      digits="(14, 2)"/>                <field name="prix_lot_livraison" digits="(14, 4)"/>           </tree>        </field>    </record>    <record id="is_comparatif_tarif_facture_search_view" model="ir.ui.view">        <field name="name">is_comparatif_tarif_facture_search_view</field>        <field name="model">is.comparatif.tarif.facture</field>        <field name="arch" type="xml">            <search string="Recherche">                <filter string="Anomalies uniquement" name="anomalies" domain="[('price_delta','!=',0)]"/>                <field name="invoice_id"/>                <field name="order_id"/>                <field name="partner_id"/>                <field name="product_id"/>            </search>        </field>    </record>    <record id="is_comparatif_tarif_facture_action" model="ir.actions.act_window">        <field name="name">Comparatif Prix Liste de prix / Facture brouillon</field>        <field name="res_model">is.comparatif.tarif.facture</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>        <field name="context">{"search_default_anomalies":1}</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouv√©e</h3>            </p>        </field>    </record></data></openerp>