<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_comparatif_lot_appro_prix_tree_view" model="ir.ui.view">        <field name="name">is_comparatif_lot_appro_prix_tree_view</field>        <field name="model">is.comparatif.lot.appro.prix</field>        <field name="arch" type="xml">            <tree create="false">                <field name="product_id"           widget="many2one_clickable"/>                <field name="is_category_id"/>                <field name="is_gestionnaire_id"/>                <field name="partner_id"           widget="many2one_clickable"/>                <field name="pricelist_id"         widget="many2one_clickable"/>                <field name="uom_id"/>                <field name="uom_po_id"/>                <field name="coef"/>                <field name="lot_mini_product"/>                <field name="min_quantity_pricelist"/>                <field name="prix_lot"/>           </tree>        </field>    </record>    <record id="is_comparatif_lot_appro_prix_form_view" model="ir.ui.view">        <field name="name">is_comparatif_lot_appro_prix_form_view</field>        <field name="model">is.comparatif.lot.appro.prix</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="product_id"/>                            <field name="is_category_id"/>                            <field name="is_gestionnaire_id"/>                            <field name="partner_id"/>                            <field name="pricelist_id"/>                            <field name="uom_id"/>                            <field name="uom_po_id"/>                            <field name="coef"/>                            <field name="lot_mini_product"/>                            <field name="min_quantity_pricelist"/>                            <field name="prix_lot"/>                        </group>                     </group>                </sheet>           </form>        </field>    </record>    <record id="is_comparatif_lot_appro_prix_search_view" model="ir.ui.view">        <field name="name">is_comparatif_lot_appro_prix_search_view</field>        <field name="model">is.comparatif.lot.appro.prix</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id"/>                <field name="partner_id"/>                <field name="is_category_id"/>                <field name="is_gestionnaire_id"/>            </search>        </field>    </record>    <record id="is_comparatif_lot_appro_prix_action" model="ir.actions.act_window">        <field name="name">Comparatif Lot d'appro et mini liste de prix</field>        <field name="res_model">is.comparatif.lot.appro.prix</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>        <field name="context">{}</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouv√©e</h3>            </p>        </field>    </record></data></openerp>