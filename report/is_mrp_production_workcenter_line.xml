<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_mrp_production_workcenter_line_form_view" model="ir.ui.view">        <field name="name">is_mrp_production_workcenter_line_form_view</field>        <field name="model">is.mrp.production.workcenter.line</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="name"/>                            <field name="product_id"/>                            <field name="state"/>                            <field name="mpwl_id"/>                            <field name="workcenter_id"/>                            <field name="sequence"/>                            <field name="hour"/>                            <field name="cycle"/>                            <field name="is_ordre"/>                            <field name="is_date_tri"/>                            <field name="is_date_planning"/>                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_mrp_production_workcenter_line_tree_view" model="ir.ui.view">        <field name="name">is_mrp_production_workcenter_line_tree_view</field>        <field name="model">is.mrp.production.workcenter.line</field>        <field name="arch" type="xml">            <tree create="false">                <field name="name"          widget="many2one_clickable"/>                <field name="product_id"    widget="many2one_clickable"/>                <field name="mpwl_id"       widget="many2one_clickable"/>                <field name="workcenter_id" widget="many2one_clickable"/>                <field name="hour"/>                <field name="cycle"/>                <field name="is_ordre"/>                <field name="is_date_planning"/>           </tree>        </field>    </record>    <record id="is_mrp_production_workcenter_line_search_view" model="ir.ui.view">        <field name="name">is_mrp_production_workcenter_line_search_view</field>        <field name="model">is.mrp.production.workcenter.line</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="name"/>                <field name="product_id"/>                <field name="state"/>                <field name="mpwl_id"/>                <field name="workcenter_id"/>                <group expand="0" string="Regrouper par...">                    <filter string="Article"          context="{'group_by':'product_id'}"/>                    <filter string="Etat"             context="{'group_by':'state'}"/>                    <filter string="Poste de charge"  context="{'group_by':'workcenter_id'}"/>                </group>            </search>        </field>    </record>    <record id="is_mrp_production_workcenter_line_action" model="ir.actions.act_window">        <field name="name">Ordres de travaux</field>        <field name="res_model">is.mrp.production.workcenter.line</field>        <field name="domain">[('state','=', 'in_production')]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record></data></openerp>