<?xml version="1.0" encoding="utf-8"?><openerp><data>    <!-- Corriger anomalie -->    <record id="corriger_anomalie_pricelist_action_server" model="ir.actions.server">         <field name="name">Corriger anomalie</field>        <field name="model_id" ref="model_is_pricelist_item"/>        <field name="code">action = self.corriger_anomalie_pricelist(cr, uid, context.get('active_ids', []), context=context)</field>    </record>    <record id="corriger_anomalie_pricelist_ir_value" model="ir.values">        <field eval="'client_action_multi'" name="key2"/>        <field eval="'is.pricelist.item'" name="model"/>        <field name="name">Corriger anomalie</field>        <field eval="'ir.actions.server,%d'%corriger_anomalie_pricelist_action_server" name="value"/>    </record>        <record id="is_pricelist_item_form_view" model="ir.ui.view">        <field name="name">is_pricelist_item_form_view</field>        <field name="model">is.pricelist.item</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="pricelist_name"/>                            <field name="price_version_id"/>                            <field name="version_date_start"/>                            <field name="version_date_end"/>                            <field name="product_id"/>                            <field name="gestionnaire_id" />                            <field name="ref_fournisseur"/>                            <field name="moule" />                            <field name="sequence"/>                            <field name="min_quantity"           digits="(14, 4)" />                            <field name="product_uom_id"/>                            <field name="product_po_uom_id"/>                            <field name="price_surcharge"        digits="(14, 4)" />                            <field name="item_date_start"/>                            <field name="item_date_end"/>                        </group>                        <group>                            <field name="pricelist_type"/>                            <field name="base"/>                        </group>                    </group>                </sheet>           </form>        </field>    </record>    <record id="is_pricelist_item_search_view" model="ir.ui.view">        <field name="name">is_pricelist_item_search_view</field>        <field name="model">is.pricelist.item</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="pricelist_name" />                <field name="pricelist_type" />                <field name="price_version_id" />                <field name="product_id" />                <field name="gestionnaire_id" />                <field name="ref_client" />                <field name="ref_fournisseur" />                <field name="moule" />                <filter string="Anomalies" domain="['|','&amp;',('pricelist_type','=','purchase'),('base','!=',2),'&amp;',('pricelist_type','=','sale'),('base','!=',1)]"/>                <group expand="1" string="Regrouper par">                    <filter string="Liste de prix"  context="{'group_by':'pricelist_name'}"/>                    <filter string="Version"        context="{'group_by':'price_version_id'}"/>                    <filter string="Article"        context="{'group_by':'product_id'}"/>                </group>            </search>        </field>    </record>    <record id="is_pricelist_item_purchase_tree_view" model="ir.ui.view">        <field name="name">is_pricelist_item_tree_view</field>        <field name="model">is.pricelist.item</field>        <field name="arch" type="xml">            <tree create="false">                <field name="pricelist_name"/>                <button string="Liste des prix" name= "action_liste_items" type="object" icon="gtk-justify-fill"/>                <field name="price_version_id"     widget="many2one_clickable"/>                <field name="version_date_start"/>                <field name="version_date_end"/>                <field name="product_id"           widget="many2one_clickable"/>                <field name="gestionnaire_id" />                <field name="ref_fournisseur"/>                <field name="moule" />                <field name="sequence"/>                <field name="min_quantity"            digits="(14, 4)" />                <field name="product_po_uom_id"/>                <field name="price_surcharge"         digits="(14, 4)" />                <field name="item_date_start"/>                <field name="item_date_end"/>                <field name="base"/>           </tree>        </field>    </record>    <record id="is_pricelist_item_purchase_action" model="ir.actions.act_window">        <field name="name">Liste de prix</field>        <field name="res_model">is.pricelist.item</field>        <field name="view_id" ref="is_pricelist_item_purchase_tree_view"/>        <field name="domain">[('pricelist_type', '=', 'purchase')]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record>    <record id="is_pricelist_item_sale_tree_view" model="ir.ui.view">        <field name="name">is_pricelist_item_tree_view</field>        <field name="model">is.pricelist.item</field>        <field name="arch" type="xml">            <tree create="false">                <field name="pricelist_name" />                <button string="Liste des prix" name= "action_liste_items" type="object" icon="gtk-justify-fill"/>                <field name="price_version_id"     widget="many2one_clickable"/>                <field name="version_date_start"/>                <field name="version_date_end" />                <field name="product_id"           widget="many2one_clickable"/>                <field name="gestionnaire_id" />                <field name="ref_client"/>                <field name="moule"/>                <field name="sequence"/>                <field name="min_quantity"         digits="(14, 4)" />                <field name="product_uom_id"/>                <field name="price_surcharge"      digits="(14, 4)" />                <field name="item_date_start"/>                <field name="item_date_end"/>                <field name="base"/>           </tree>        </field>    </record>    <record id="is_pricelist_item_sale_action" model="ir.actions.act_window">        <field name="name">Liste de prix</field>        <field name="res_model">is.pricelist.item</field>        <field name="view_id" ref="is_pricelist_item_sale_tree_view"/>        <field name="domain">[('pricelist_type', '=', 'sale')]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record></data></openerp>