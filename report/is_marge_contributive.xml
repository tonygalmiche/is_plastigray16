<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_marge_contributive_form_view" model="ir.ui.view">        <field name="name">is_marge_contributive_form_view</field>        <field name="model">is.marge.contributive</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="line_id"/>                            <field name="invoice_id"/>                            <field name="date_invoice"/>                            <field name="code_pg"/>                            <field name="gest"/>                            <field name="segment"/>                            <field name="fam"/>                            <field name="moule"/>                            <field name="partner_id"/>                            <field name="client_fac"/>                            <field name="designation"/>                            <field name="amortissement_moule"/>                            <field name="cout_std_matiere"/>                            <field name="cout_std_machine"/>                            <field name="cout_std_mo"/>                            <field name="cout_std_st"/>                            <field name="cout_std_prix_vente"/>                            <field name="cout_act_matiere"/>                            <field name="cout_act_machine"/>                            <field name="cout_act_mo"/>                            <field name="cout_act_st"/>                            <field name="product_id"/>                            <field name="quantity"/>                            <field name="price_unit"/>                            <field name="montant"/>                            <field name="ca_prix_vente_std"/>                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_marge_contributive_tree_view" model="ir.ui.view">        <field name="name">is_marge_contributive_tree_view</field>        <field name="model">is.marge.contributive</field>        <field name="arch" type="xml">            <tree>                <field name="invoice_id"  widget="many2one_clickable"/>                <field name="client_fac"/>                <field name="raison_sociale"/>                <field name="date_invoice"/>                <field name="code_pg"/>                <field name="designation"/>                <field name="moule"/>                <field name="amortissement_moule"/>                <field name="cout_std_matiere"/>                <field name="cout_std_machine"/>                <field name="cout_std_mo"/>                <field name="cout_std_st"/>                <field name="cout_std_prix_vente"/>                <field name="cout_act_matiere"/>                <field name="cout_act_machine"/>                <field name="cout_act_mo"/>                <field name="cout_act_st"/>                <field name="quantity"          sum="Total"/>                <field name="price_unit"/>                <field name="montant"           sum="Total"/>                <field name="ca_prix_vente_std" sum="Total"/>           </tree>        </field>    </record>    <record id="is_marge_contributive_search_view" model="ir.ui.view">        <field name="name">is_marge_contributive_search_view</field>        <field name="model">is.marge.contributive</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="partner_id"/>                <field name="product_id"/>                <group expand="1" string="Regrouper par...">                    <filter string="Code PG"  context="{'group_by':'code_pg'}"/>                    <filter string="Client  " context="{'group_by':'client_fac'}"/>                    <filter string="Mois"     context="{'group_by':'date_invoice:month'}"/>                </group>            </search>        </field>    </record>    <record id="is_marge_contributive_graph_view" model="ir.ui.view">        <field name="name">is_marge_contributive_graph_view</field>        <field name="model">is.marge.contributive</field>        <field name="arch" type="xml">            <graph type="pivot">                <field name="code_pg"/>                <field name="date_invoice"       type="col"/>                <field name="montant"            type="measure"/>            </graph>        </field>    </record>    <record id="is_marge_contributive_action" model="ir.actions.act_window">        <field name="name">Marge contributive sur facture</field>        <field name="res_model">is.marge.contributive</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form,graph</field>    </record></data></openerp>