<?xml version="1.0" encoding="utf-8"?><openerp><data>        <!-- Bloquer lot -->    <record id="view_stock_lot_bloque_tree" model="ir.ui.view">        <field name="name">report.stock.lot.bloque.tree</field>        <field name="model">report.stock.lot.bloque</field>        <field name="arch" type="xml">            <tree string="Bloquer lots" create="false">                <field name="product_id" />                <field name="mold"/>                <field name="location_id"/>                <field name="lot_id" />                <field name="qty" sum="Total" />                <field name="qty_par_uc" />                <field name="qty_uc" sum="Total"/>                <field name="in_date" />                <field name="operation" invisible="1"/>                <button string="Bloquer lot"                        type="action"                        name="%(action_is_gestion_lot)d"                        icon="gtk-index" />                <button string="Mise au rebut"                        type="action"                        name="%(action_is_mise_rebut)d"                        icon="terp-gtk-jump-to-ltr"                         groups="is_plastigray.is_rsp_production_group,is_plastigray.is_qualite_group"/>           </tree>        </field>    </record>        <record id="view_stock_lot_bloque_search" model="ir.ui.view">        <field name="name">report.stock.lot.bloque.search</field>        <field name="model">report.stock.lot.bloque</field>        <field name="arch" type="xml">            <search string="Bloquer lot">                <field name="product_id" />                <field name="mold" />                <field name="lot_id" />                <field name="location_id" />                <group expand="1" string="Group By...">                    <filter name="group_product"  string="Article"     icon="terp-accessories-archiver" context="{'group_by':'product_id'}"/>                    <filter name="group_mold"  string="Moule  "        icon="terp-accessories-archiver" context="{'group_by':'mold'}"/>                    <filter name="group_lot"      string="Lot"         icon="terp-accessories-archiver" context="{'group_by':'lot_id'}"/>                    <filter name="group_location" string="Emplacement" icon="terp-accessories-archiver" context="{'group_by':'location_id'}"/>                </group>            </search>        </field>    </record>    <record id="action_stock_lot_bloque_report" model="ir.actions.act_window">        <field name="name">Bloquer lot</field>        <field name="res_model">report.stock.lot.bloque</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>        <field name="context">{'operation': 'bloque'}</field>        <field name="search_view_id" ref="view_stock_lot_bloque_search" />    </record></data></openerp>