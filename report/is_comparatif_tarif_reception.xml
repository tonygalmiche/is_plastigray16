<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_comparatif_tarif_reception_tree_view" model="ir.ui.view">        <field name="name">is_comparatif_tarif_reception_tree_view</field>        <field name="model">is.comparatif.tarif.reception</field>        <field name="arch" type="xml">            <tree>                <field name="order_id"           widget="many2one_clickable"  />                <field name="date_approve" />                <field name="partner_id"         widget="many2one_clickable"  />                <field name="pricelist_id"       widget="many2one_clickable"  />                <field name="product_id"         widget="many2one_clickable"  />                <field name="qty"                digits="(14, 4)"/>                <field name="date_planned" />                <field name="justification" />                <field name="pol_price"          digits="(14, 4)"/>                <field name="pol_uom_id"/>                <field name="pricelist_price"    digits="(14, 4)"/>                <field name="pricelist_uom_id" />                <field name="factor"             digits="(14, 6)"/>                <field name="price_delta"        digits="(14, 4)"/>           </tree>        </field>    </record>    <record id="is_comparatif_tarif_reception_search_view" model="ir.ui.view">        <field name="name">is_comparatif_tarif_reception_search_view</field>        <field name="model">is.comparatif.tarif.reception</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="order_id"/>                <field name="partner_id"/>                <field name="product_id"/>            </search>        </field>    </record>    <record id="is_comparatif_tarif_reception_action" model="ir.actions.act_window">        <field name="name">Comparatif Prix Liste de prix / Réception en cours</field>        <field name="res_model">is.comparatif.tarif.reception</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouvée</h3>            </p>        </field>    </record></data></openerp>