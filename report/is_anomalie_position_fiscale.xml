<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_anomalie_position_fiscale_form_view" model="ir.ui.view">        <field name="name">is_anomalie_position_fiscale_form_view</field>        <field name="model">is.anomalie.position.fiscale</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="partner_id" />                            <field name="order_id" />                            <field name="product_id" />                            <field name="product_uom" />                            <field name="product_qty_uom_po" />                            <field name="qt_rcp" />                            <field name="qt_reste" />                            <field name="position_id" />                            <field name="fiscal_position" />                        </group>                     </group>                </sheet>            </form>        </field>    </record>    <record id="is_anomalie_position_fiscale_tree_view" model="ir.ui.view">        <field name="name">is_anomalie_position_fiscale_tree_view</field>        <field name="model">is.anomalie.position.fiscale</field>        <field name="arch" type="xml">            <tree create="false">                <field name="partner_id"           widget="many2one_clickable"/>                <field name="order_id"             widget="many2one_clickable"/>                <field name="product_id"          widget="many2one_clickable" />                <field name="product_uom" />                <field name="product_qty_uom_po" />                <field name="qt_rcp" />                <field name="qt_reste" />                <field name="position_id" />                <field name="fiscal_position" />           </tree>        </field>    </record>    <record id="is_anomalie_position_fiscale_search_view" model="ir.ui.view">        <field name="name">is_anomalie_position_fiscale_search_view</field>        <field name="model">is.anomalie.position.fiscale</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="partner_id" />                <field name="order_id" />                <field name="product_id" />            </search>        </field>    </record>    <record id="is_anomalie_position_fiscale_action" model="ir.actions.act_window">        <field name="name">Anomalies position fiscale fournisseur</field>        <field name="res_model">is.anomalie.position.fiscale</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouv√©e</h3>            </p>        </field>    </record></data></openerp>