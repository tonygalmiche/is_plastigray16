<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_article_sans_fournisseur_tree_view" model="ir.ui.view">        <field name="name">is_article_sans_fournisseur_tree_view</field>        <field name="model">is.article.sans.fournisseur</field>        <field name="arch" type="xml">            <tree create="false">                <field name="product_id"     widget="many2one_clickable" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <field name="nb" />           </tree>        </field>    </record>    <record id="is_article_sans_fournisseur_search_view" model="ir.ui.view">        <field name="name">is_article_sans_fournisseur_search_view</field>        <field name="model">is.article.sans.fournisseur</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="is_code" />                <field name="designation" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />            </search>        </field>    </record>    <record id="is_article_sans_fournisseur_action" model="ir.actions.act_window">        <field name="name">Articles achetés (route=Buy) sans fournisseur dans l'onglet 'Approvisionnement'</field>        <field name="res_model">is.article.sans.fournisseur</field>        <field name="domain">[('nb','=',0)]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouvée</h3>            </p>        </field>    </record></data></openerp>