<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_certifications_qualite_form_view" model="ir.ui.view">        <field name="name">is_certifications_qualite_form_view</field>        <field name="model">is.certifications.qualite</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <field name="partner_id"/>                        <field name="is_norme" />                        <field name="is_date_validation" />                        <field name="is_certificat_ids"  widget="many2many_binary"/>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_certifications_qualite_suivi_form_view" model="ir.ui.view">        <field name="name">is_certifications_qualite_suivi_form_view</field>        <field name="model">is.certifications.qualite.suivi</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="partner_id"/>                            <field name="is_segment_achat"/>                            <field name="certificat_id"/>                            <field name="is_date_validation"/>                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_certifications_qualite_suivi_tree_view" model="ir.ui.view">        <field name="name">is_certifications_qualite_suivi_tree_view</field>        <field name="model">is.certifications.qualite.suivi</field>        <field name="arch" type="xml">            <tree create="false">                <field name="partner_id"           widget="many2one_clickable"/>                <field name="is_segment_achat"/>                <field name="certificat_id"        widget="many2one_clickable"/>                <field name="is_date_validation"/>           </tree>        </field>    </record>    <record id="is_certifications_qualite_suivi_search_view" model="ir.ui.view">        <field name="name">is_certifications_qualite_suivi_search_view</field>        <field name="model">is.certifications.qualite.suivi</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="partner_id"/>                <field name="is_segment_achat"/>                <field name="is_norme"/>            </search>        </field>    </record>    <record id="is_certifications_qualite_suivi_action" model="ir.actions.act_window">        <field name="name">Suivi validit√© des certificats</field>        <field name="res_model">is.certifications.qualite.suivi</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record></data></openerp>