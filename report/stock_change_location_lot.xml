<?xml version="1.0" encoding="utf-8"?><openerp><data>    <!--        res_model="is.change.emplacement.wizard" -->    <act_window         id="is_change_emplacement_wizard_action"         res_model="is.change.emplacement.wizard"        src_model="report.stock.lot.change.location"        target="new"         multi="True" key2="client_action_multi"         view_mode="form"         name="Changement d'emplacement"    />    <record id="view_stock_lot_change_location_tree" model="ir.ui.view">        <field name="name">report.stock.lot.change.location.tree</field>        <field name="model">report.stock.lot.change.location</field>        <field name="arch" type="xml">            <tree string="Changer emplacement" create="false">                <field name="product_id"/>                <field name="mold"/>                <field name="location_id"/>                <field name="lot_id" />                <field name="qty" sum="Total"/>                <field name="qty_par_uc" />                <field name="qty_uc" sum="Total"/>                <field name="in_date" />                <field name="operation" invisible="1"/>                <button string="Changer d'emplacement"                        type="action"                        name="%(action_is_gestion_lot)d"                        icon="gtk-index" />                <!--<button string="Mise au rebut"                        type="action"                        name="%(action_is_mise_rebut)d"                        icon="terp-gtk-jump-to-ltr" />-->           </tree>        </field>    </record>        <record id="view_stock_lot_change_location_search" model="ir.ui.view">        <field name="name">report.stock.lot.change.location.search</field>        <field name="model">report.stock.lot.change.location</field>        <field name="arch" type="xml">            <search string="Changer emplacement">                <field name="product_id" />                <field name="mold" />                <field name="lot_id" />                <field name="location_id" />                <group expand="1" string="Group By...">                    <filter name="group_product"  string="Article"     icon="terp-accessories-archiver" context="{'group_by':'product_id'}"/>                    <filter name="group_mold"     string="Moule  "     icon="terp-accessories-archiver" context="{'group_by':'mold'}"/>                    <filter name="group_lot"      string="Lot"         icon="terp-accessories-archiver" context="{'group_by':'lot_id'}"/>                    <filter name="group_location" string="Emplacement" icon="terp-accessories-archiver" context="{'group_by':'location_id'}"/>                </group>            </search>        </field>    </record>    <record id="action_stock_lot_change_location_report" model="ir.actions.act_window">        <field name="name">Changement d'emplacement</field>        <field name="res_model">report.stock.lot.change.location</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>        <field name="search_view_id" ref="view_stock_lot_change_location_search" />    </record></data></openerp>