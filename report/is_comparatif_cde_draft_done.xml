<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_comparatif_cde_draft_done_tree_view" model="ir.ui.view">        <field name="name">is_comparatif_cde_draft_done_tree_view</field>        <field name="model">is.comparatif.cde.draft.done</field>        <field name="arch" type="xml">            <tree create="false">                <field name="product_id"          widget="many2one_clickable" />                <field name="date_livraison" />                <field name="order_id"            widget="many2one_clickable" />                <field name="qt_commande"    />                <field name="qt_livree" />           </tree>        </field>    </record>    <record id="is_comparatif_cde_draft_done_search_view" model="ir.ui.view">        <field name="name">is_comparatif_cde_draft_done_search_view</field>        <field name="model">is.comparatif.cde.draft.done</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id" />                <field name="order_id" />            </search>        </field>    </record>    <record id="is_comparatif_cde_draft_done_action" model="ir.actions.act_window">        <field name="name">Comparatif Commande en cours / Commande livrée</field>        <field name="res_model">is.comparatif.cde.draft.done</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouvée</h3>            </p>        </field>    </record></data></openerp>