<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_comparatif_uc_lot_tree_view" model="ir.ui.view">        <field name="name">is_comparatif_uc_lot_tree_view</field>        <field name="model">is.comparatif.uc.lot</field>        <field name="arch" type="xml">            <tree create="false">                <field name="product_id"  widget="many2one_clickable" />                <field name="partner_id"  widget="many2one_clickable" />                <field name="is_category_id"    />                <field name="uc" />                <field name="lot_livraison" />                <field name="multiple_livraison" />                <field name="test_lot" />                <field name="test_multiple" />           </tree>        </field>    </record>    <record id="is_comparatif_uc_lot_search_view" model="ir.ui.view">        <field name="name">is_comparatif_uc_lot_search_view</field>        <field name="model">is.comparatif.uc.lot</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id" />                <field name="partner_id" />                <field name="is_category_id" />            </search>        </field>    </record>    <record id="is_comparatif_uc_lot_action" model="ir.actions.act_window">        <field name="name">Comparatif UC / Lot</field>        <field name="res_model">is.comparatif.uc.lot</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouv√©e</h3>            </p>        </field>    </record></data></openerp>