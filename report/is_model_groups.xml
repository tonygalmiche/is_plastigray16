<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_model_groups_form_view" model="ir.ui.view">        <field name="name">is_model_groups_form_view</field>        <field name="model">is.model.groups</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <field name="service" />                        <field name="login" />                        <field name="user_id" />                        <field name="email" />                        <field name="category" />                        <field name="group" />                        <field name="active_user" />                        <field name="active_group" />                        <field name="model_id" />                        <field name="model_name" />                        <field name="perm_read" />                        <field name="perm_write" />                        <field name="perm_create" />                        <field name="perm_unlink" />                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_model_groups_tree_view" model="ir.ui.view">        <field name="name">is_model_groups_tree_view</field>        <field name="model">is.model.groups</field>        <field name="arch" type="xml">            <tree create="false">                <field name="service" />                <field name="login" />                <field name="user_id" />                <field name="email" />                <field name="category"  />                <field name="group"     />                <field name="active_user" />                <field name="active_group" />                <field name="model_id" />                <field name="model_name" />                <field name="perm_read" />                <field name="perm_write" />                <field name="perm_create" />                <field name="perm_unlink" />           </tree>        </field>    </record>    <record id="is_model_groups_search_view" model="ir.ui.view">        <field name="name">is_model_groups_search_view</field>        <field name="model">is.model.groups</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="user_id" />                <field name="email" />                <field name="login" />                <field name="service" />                <field name="group" />                <field name="category" />            </search>        </field>    </record>    <record id="is_model_groups_graph" model="ir.ui.view">         <field name="name">is_model_groups_graph</field>         <field name="model">is.model.groups</field>         <field name="arch" type="xml">             <graph type="pivot">                 <field name="model_id" type="row"/>                 <field name="service"  type="col"/>                 <field name="login"    type="col"/>             </graph>         </field>    </record>    <record id="is_model_groups_action" model="ir.actions.act_window">        <field name="name">Mod√®les / Groupes</field>        <field name="res_model">is.model.groups</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">graph,tree,form</field>        <field name="view_id" ref=""/>    </record></data></openerp>