<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_mouvement_stock_form_view" model="ir.ui.view">        <field name="name">s_mouvement_stock_form_view</field>        <field name="model">is.mouvement.stock</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <field name="product_id" />                        <field name="quant_qty" />                        <field name="move_qty" />                        <field name="product_uom" />                        <field name="location_id" />                        <field name="location_dest_id" />                        <field name="date" />                        <field name="origin" />                        <field name="lot_id"  />                        <field name="move_id"  />                        <field name="quant_id"/>                        <field name="createur"  />                    </group>                </sheet>            </form>        </field>    </record>        <record id="is_mouvement_stock_tree_view" model="ir.ui.view">        <field name="name">is_mouvement_stock_tree_view</field>        <field name="model">is.mouvement.stock</field>        <field name="arch" type="xml">            <tree create="false">                <field name="product_id"     widget="many2one_clickable" />                <field name="quant_qty" />                <field name="move_qty" />                <field name="product_uom" />                <field name="location_id" />                <field name="location_dest_id" />                <field name="date" />                <field name="origin" />                <field name="lot_id"   widget="many2one_clickable" />                <field name="move_id"  widget="many2one_clickable" />                <field name="quant_id" widget="many2one_clickable" />                <field name="createur" widget="many2one_clickable" />           </tree>        </field>    </record>    <record id="is_mouvement_stock_search_view" model="ir.ui.view">        <field name="name">is_mouvement_stock_search_view</field>        <field name="model">is.mouvement.stock</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id"/>                <field name="location_id" />                <field name="location_dest_id" />                <field name="origin" />                <field name="lot_id"  />                <field name="createur" />            </search>        </field>    </record>    <record id="is_mouvement_stock_action" model="ir.actions.act_window">        <field name="name">Mouvements de stocks</field>        <field name="res_model">is.mouvement.stock</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record></data></openerp>