<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_nomenclature_sans_gamme_tree_view" model="ir.ui.view">        <field name="name">is_nomenclature_sans_gamme_tree_view</field>        <field name="model">is.nomenclature.sans.gamme</field>        <field name="arch" type="xml">            <tree create="false">                <field name="bom_id"                widget="many2one_clickable" />                <field name="is_sous_traitance" />                <field name="is_negoce" />                <field name="is_inactive" />                <field name="product_id"            widget="many2one_clickable" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <field name="routing_id"            widget="many2one_clickable" />                <field name="is_gamme_generique_id" widget="many2one_clickable" />           </tree>        </field>    </record>    <record id="is_nomenclature_sans_gamme_search_view" model="ir.ui.view">        <field name="name">is_nomenclature_sans_gamme_search_view</field>        <field name="model">is.nomenclature.sans.gamme</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id" />                <field name="bom_id" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />            </search>        </field>    </record>    <record id="is_nomenclature_sans_gamme_action" model="ir.actions.act_window">        <field name="name">Nomenclatures sans gamme</field>        <field name="res_model">is.nomenclature.sans.gamme</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouv√©e</h3>            </p>        </field>    </record></data></openerp>