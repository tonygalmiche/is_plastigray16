<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_pic_3mois_tree_view" model="ir.ui.view">        <field name="name">is_pic_3mois_tree_view</field>        <field name="model">is.pic.3mois</field>        <field name="arch" type="xml">            <tree>                <field name="partner_id" />                <field name="is_mold_id" />                <field name="project" />                <field name="product_id" />                <field name="is_category_id" />                <field name="description" />                <field name="is_ref_client" />                <field name="product_uom_qty" />                <field name="is_date_expedition" />                <field name="is_type_commande" />                <field name="route_id" />           </tree>        </field>    </record>    <record id="is_pic_3mois_graph_view" model="ir.ui.view">        <field name="name">is_pic_3mois_graph_view</field>        <field name="model">is.pic.3mois</field>        <field name="arch" type="xml">            <graph type="pivot">                <field name="partner_id"  type="row"/>                <field name="description" type="row"/>                <field name="is_date_expedition" type="col"  interval="week"/>                <field name="product_uom_qty" type="measure"/>            </graph>        </field>    </record>    <record id="is_pic_3mois_search_view" model="ir.ui.view">        <field name="name">is_pic_3mois_search_view</field>        <field name="model">is.pic.3mois</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="partner_id"/>                <field name="is_mold_id" />                <field name="project" />                <field name="product_id" />            </search>        </field>    </record>    <record id="is_pic_3mois_action" model="ir.actions.act_window">        <field name="name">PIC Ã  3 mois</field>        <field name="res_model">is.pic.3mois</field>        <field name="view_type">form</field>        <field name="view_mode">graph,tree</field>    </record></data></openerp>