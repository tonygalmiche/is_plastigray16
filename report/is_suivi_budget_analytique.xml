<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_suivi_budget_analytique_form_view" model="ir.ui.view">        <field name="name">is_suivi_budget_analytique_form_view</field>        <field name="model">is.suivi.budget.analytique</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="product_id"/>                            <field name="move_id"/>                            <field name="order_id"/>                            <field name="partner_id"/>                            <field name="account_section"/>                            <field name="date_planned"/>                            <field name="montant"/>                            <field name="product_uom_qty"/>                            <field name="price_unit"/>                            <field name="pol_uom"/>                            <field name="sm_uom"/>                            <field name="coef"/>                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_suivi_budget_analytique_tree_view" model="ir.ui.view">        <field name="name">is_suivi_budget_analytique_tree_view</field>        <field name="model">is.suivi.budget.analytique</field>        <field name="arch" type="xml">            <tree>                <field name="product_id"          widget="many2one_clickable"/>                <field name="order_id"            widget="many2one_clickable"/>                <field name="partner_id"          widget="many2one_clickable"/>                <field name="account_section"/>                <field name="date_planned"/>                <field name="product_uom_qty"     sum="Total" />                <field name="price_unit"/>                <field name="montant"             sum="Total"/>                <field name="pol_uom"/>                <field name="sm_uom"/>                <field name="coef"/>           </tree>        </field>    </record>    <record id="is_suivi_budget_analytique_search_view" model="ir.ui.view">        <field name="name">is_suivi_budget_analytique_search_view</field>        <field name="model">is.suivi.budget.analytique</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id"/>                <field name="order_id"/>                <field name="partner_id"/>                <field name="account_section"/>                <group expand="1" string="Regrouper par...">                    <filter string="Fournisseur"     context="{'group_by':'partner_id'}"/>                    <filter string="Article"         context="{'group_by':'product_id'}"/>                    <filter string="Compte-Section"  context="{'group_by':'account_section'}"/>                    <filter string="Mois"            context="{'group_by':'date_planned:month'}"/>                </group>            </search>        </field>    </record>    <record id="is_suivi_budget_analytique_graph_view" model="ir.ui.view">        <field name="name">is_suivi_budget_analytique_graph_view</field>        <field name="model">is.suivi.budget.analytique</field>        <field name="arch" type="xml">            <graph type="pivot">                <field name="product_id"/>                <field name="date_planned"       type="col"/>                <field name="montant"            type="measure"/>            </graph>        </field>    </record>    <record id="is_suivi_budget_analytique_action" model="ir.actions.act_window">        <field name="name">Suivi budget analytique</field>        <field name="res_model">is.suivi.budget.analytique</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form,graph</field>    </record></data></openerp>