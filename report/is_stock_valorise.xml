<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_stock_valorise_tree_view" model="ir.ui.view">        <field name="name">is_stock_valorise_tree_view</field>        <field name="model">is.stock.valorise</field>        <field name="arch" type="xml">            <tree create="false">                <field name="is_code" />                <field name="designation" />                <field name="segment_id" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <field name="stock_a" sum="Total"/>                <field name="stock_q" sum="Total"/>                <field name="stock"   sum="Total"/>                <field name="cout_act_matiere" />                <field name="cout_act_machine" />                <field name="cout_act_mo" />                <field name="cout_act_st" />                <field name="cout_act_total" />                <field name="stock_valorise" sum="Total"/>           </tree>        </field>    </record>    <record id="is_stock_valorise_search_view" model="ir.ui.view">        <field name="name">is_stock_valorise_search_view</field>        <field name="model">is.stock.valorise</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="is_code" />                <field name="designation" />                <field name="segment_id" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <group expand="0" string="Regrouper par...">                    <filter string="Segment"      context="{'group_by':'segment_id'}"/>                    <filter string="Catégorie"    context="{'group_by':'is_category_id'}"/>                    <filter string="Gestionnaire" context="{'group_by':'is_gestionnaire_id'}"/>                </group>            </search>        </field>    </record>    <record id="is_stock_valorise_action" model="ir.actions.act_window">        <field name="name">Liste des stocks valorisés</field>        <field name="res_model">is.stock.valorise</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record></data></openerp>