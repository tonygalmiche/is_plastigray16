<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_article_sans_cde_ouverte_fou_form_view" model="ir.ui.view">        <field name="name">is_article_sans_cde_ouverte_fou_form_view</field>        <field name="model">is.article.sans.cde.ouverte.fou</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="product_id" />                            <field name="is_category_id" />                            <field name="is_gestionnaire_id" />                            <field name="product_nb_fournisseurs" />                            <field name="product_partner_id" />                            <field name="nb_cde_ouverte" />                            <field name="cde_ouverte_partner_id" />                        </group>                     </group>                </sheet>            </form>        </field>    </record>    <record id="is_article_sans_cde_ouverte_fou_tree_view" model="ir.ui.view">        <field name="name">is_article_sans_cde_ouverte_fou_tree_view</field>        <field name="model">is.article.sans.cde.ouverte.fou</field>        <field name="arch" type="xml">            <tree create="false">                <field name="product_id"             widget="many2one_clickable"/>                <field name="is_category_id"/>                <field name="is_gestionnaire_id" />                <field name="product_nb_fournisseurs"/>                <field name="product_partner_id"     widget="many2one_clickable"/>                <field name="nb_cde_ouverte"/>                <field name="cde_ouverte_partner_id" widget="many2one_clickable"/>           </tree>        </field>    </record>    <record id="is_article_sans_cde_ouverte_fou_search_view" model="ir.ui.view">        <field name="name">is_article_sans_cde_ouverte_fou_search_view</field>        <field name="model">is.article.sans.cde.ouverte.fou</field>        <field name="arch" type="xml">            <search string="Recherche">                <filter string="Sans commande ouverte" domain="[('nb_cde_ouverte', '=', 0)]" name="sans_cde_ouverte"/>                <field name="product_id" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <field name="product_partner_id" />                <field name="cde_ouverte_partner_id" />                <group expand="0" string="Regrouper par...">                    <filter string="Catégorie"        context="{'group_by':'is_category_id'}"/>                    <filter string="Gestionnaire"     context="{'group_by':'is_gestionnaire_id'}"/>                    <filter string="Fournisseur"      context="{'group_by':'product_partner_id'}"/>                </group>            </search>        </field>    </record>    <record id="is_article_sans_cde_ouverte_fou_action" model="ir.actions.act_window">        <field name="name">Articles achetés sans commande ouverte</field>        <field name="res_model">is.article.sans.cde.ouverte.fou</field>        <field name="domain">[]</field>        <field name="context">{"search_default_sans_cde_ouverte":1}</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouvée</h3>            </p>        </field>    </record></data></openerp>