<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_anomalie_declar_prod_form_view" model="ir.ui.view">        <field name="name">is_anomalie_declar_prod_form_view</field>        <field name="model">is.anomalie.declar.prod</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="mp_id" />                            <field name="compose_id" />                            <field name="mp_state" />                            <field name="composant_id" />                            <field name="cat" />                            <field name="mppl_is_bom_qty" />                            <field name="qt_pf" />                            <field name="qt_rebuts" />                            <field name="qt_composant" />                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_anomalie_declar_prod_tree_view" model="ir.ui.view">        <field name="name">is_anomalie_declar_prod_tree_view</field>        <field name="model">is.anomalie.declar.prod</field>        <field name="arch" type="xml">            <tree create="false">                <field name="mp_id"                widget="many2one_clickable"/>                <field name="compose_id"           widget="many2one_clickable"/>                <field name="mp_state" />                <field name="composant_id"         widget="many2one_clickable"/>                <field name="cat" />                <field name="mppl_is_bom_qty" />                <field name="qt_pf" />                <field name="qt_rebuts" />                <field name="qt_composant" />           </tree>        </field>    </record>    <record id="is_anomalie_declar_prod_search_view" model="ir.ui.view">        <field name="name">is_anomalie_declar_prod_search_view</field>        <field name="model">is.anomalie.declar.prod</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="mp_id" />                <field name="compose_id" />                <field name="mp_state" />                <field name="composant_id" />                <field name="cat" />                <filter name="is_semi_fini_filter" string="Hors cat 40, 42 et 60" domain="[('cat','not in',['40','42','60'])]"/>            </search>        </field>    </record>    <record id="is_anomalie_declar_prod_action" model="ir.actions.act_window">        <field name="name">Anomalies de d√©clarations de production</field>        <field name="res_model">is.anomalie.declar.prod</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>        <field name="context">{'search_default_is_semi_fini_filter':1}</field>    </record></data></openerp>