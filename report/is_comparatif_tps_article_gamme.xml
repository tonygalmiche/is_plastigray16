<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_comparatif_tps_article_gamme_tree_view" model="ir.ui.view">        <field name="name">is_comparatif_tps_article_gamme_tree_view</field>        <field name="model">is.comparatif.tps.article.gamme</field>        <field name="arch" type="xml">            <tree colors="red:delta_nb_secondes!=0">                <field name="product_id"     widget="many2one_clickable" />                <field name="is_category_id"     />                <field name="sequence" />                <field name="routing_id"     widget="many2one_clickable"  />                <field name="name" />                <field name="workcenter_id"  widget="many2one_clickable"  />                <field name="nb_secondes_gamme" />                <field name="nb_secondes_article" />                <field name="delta_nb_secondes" />           </tree>        </field>    </record>    <record id="is_comparatif_tps_article_gamme_search_view" model="ir.ui.view">        <field name="name">is_comparatif_tps_article_gamme_search_view</field>        <field name="model">is.comparatif.tps.article.gamme</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id" />                <field name="routing_id" />                <field name="workcenter_id" />            </search>        </field>    </record>    <record id="is_comparatif_tps_article_gamme_action" model="ir.actions.act_window">        <field name="name">Comparatif Tps Article/Gamme</field>        <field name="res_model">is.comparatif.tps.article.gamme</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record></data></openerp>