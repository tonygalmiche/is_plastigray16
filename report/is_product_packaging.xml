<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_product_packaging_form_view" model="ir.ui.view">        <field name="name">is_product_packaging_form_view</field>        <field name="model">is.product.packaging</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <field name="product_tmpl_id" />                        <field name="segment_id" />                        <field name="is_category_id" />                        <field name="is_gestionnaire_id" />                        <field name="is_mold_dossierf" />                        <field name="weight_net" />                        <field name="weight" />                        <field name="sequence" />                        <field name="qty" />                        <field name="ul" />                        <field name="ul_qty" />                        <field name="rows" />                        <field name="ul_container" />                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_product_packaging_tree_view" model="ir.ui.view">        <field name="name">is_product_packaging_tree_view</field>        <field name="model">is.product.packaging</field>        <field name="arch" type="xml">            <tree create="false">                <field name="product_tmpl_id"      widget="many2one_clickable"/>                <field name="segment_id" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <field name="is_mold_dossierf" />                <field name="weight_net"/>                <field name="weight"/>                <field name="sequence" />                <field name="qty"/>                <field name="ul" />                <field name="ul_qty"/>                <field name="rows" />                <field name="ul_container" />           </tree>        </field>    </record>    <record id="is_product_packaging_search_view" model="ir.ui.view">        <field name="name">is_product_packaging_search_view</field>        <field name="model">is.product.packaging</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_tmpl_id" />                <field name="segment_id" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <field name="is_mold_dossierf" />                <field name="ul" />                <field name="ul_container" />            </search>        </field>    </record>    <record id="is_product_packaging_action" model="ir.actions.act_window">        <field name="name">Logistique article</field>        <field name="res_model">is.product.packaging</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record></data></openerp>