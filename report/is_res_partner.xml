<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_res_partner_form_view" model="ir.ui.view">        <field name="name">is_res_partner_form_view</field>        <field name="model">is.res.partner</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="partner_id"/>                            <field name="segment_id"/>                            <field name="cde_ouverte_id"/>                            <field name="cde_ferme_cadencee_id"/>                            <field name="supplier"/>                            <field name="customer"/>                        </group>                     </group>                </sheet>            </form>        </field>    </record>    <record id="is_res_partner_tree_view" model="ir.ui.view">        <field name="name">is_res_partner_tree_view</field>        <field name="model">is.res.partner</field>        <field name="arch" type="xml">            <tree create="false">                <field name="partner_id"            widget="many2one_clickable"/>                <field name="segment_id"            widget="many2one_clickable"/>                <field name="cde_ouverte_id"        widget="many2one_clickable"/>                <field name="cde_ferme_cadencee_id" widget="many2one_clickable"/>           </tree>        </field>    </record>    <record id="is_res_partner_search_view" model="ir.ui.view">        <field name="name">is_res_partner_search_view</field>        <field name="model">is.res.partner</field>        <field name="arch" type="xml">            <search string="Recherche">                <filter string="Anomalies" domain="[('cde_ouverte_id','=', False),('cde_ferme_cadencee_id','=', False)]" name="anomalies" />                <field name="partner_id"/>                <field name="segment_id"/>                <field name="cde_ouverte_id"/>                <field name="cde_ferme_cadencee_id"/>                <group expand="0" string="Regrouper par...">                    <filter string="Segment"      context="{'group_by':'segment_id'}"/>                </group>            </search>        </field>    </record>    <record id="is_res_partner_action" model="ir.actions.act_window">        <field name="name">Fournisseurs sans commande prévisionnelle et sans commande ferme cadencée</field>        <field name="res_model">is.res.partner</field>        <field name="domain">[('supplier','=',True)]</field>        <field name="context">{"search_default_anomalies":1}</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouvée</h3>            </p>        </field>    </record></data></openerp>