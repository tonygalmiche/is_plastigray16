<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_stock_quant_form_view" model="ir.ui.view">        <field name="name">is_stock_quant_form_view</field>        <field name="model">is.stock.quant</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <field name="product_id" />                        <field name="gestionnaire_id" />                        <field name="category_id" />                        <field name="moule" />                        <field name="client_id" />                        <field name="ref_client" />                        <field name="ref_fournisseur" />                        <field name="location_id" />                        <field name="lot" />                        <field name="lot_fournisseur" />                        <field name="quantite" />                        <field name="uom_id" />                        <field name="date_entree" />                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_stock_quant_tree_view" model="ir.ui.view">        <field name="name">is_stock_quant_tree_view</field>        <field name="model">is.stock.quant</field>        <field name="arch" type="xml">            <tree>                <field name="code_pg"/>                <field name="designation" />                <field name="gestionnaire_id" />                <field name="category_id" />                <field name="moule" />                <field name="client_id"/>                <field name="ref_client" />                <field name="ref_fournisseur" />                <field name="emplacement" />                <field name="lot" />                <field name="lot_fournisseur" />                <field name="quantite" sum="Total"/>                <field name="uom_id" />                <field name="date_entree" />           </tree>        </field>    </record>    <record id="is_stock_quant_search_view" model="ir.ui.view">        <field name="name">is_stock_quant_search_view</field>        <field name="model">is.stock.quant</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id" />                <field name="gestionnaire_id" />                <field name="category_id" />                <field name="moule" />                <field name="client_id" />                <field name="ref_client" />                <field name="ref_fournisseur" />                <field name="location_id" />                <field name="lot" />                <field name="lot_fournisseur" />                <group string="Regrouper par">                    <filter string="Article"        context="{'group_by': 'product_id'}"/>                    <filter string="Emplacement"    context="{'group_by': 'location_id'}"/>                    <filter string="Client"         context="{'group_by': 'client_id'}"/>                </group>            </search>        </field>    </record>    <record id="is_stock_quant_action" model="ir.actions.act_window">        <field name="name">Stock détaillé</field>        <field name="res_model">is.stock.quant</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record></data></openerp>